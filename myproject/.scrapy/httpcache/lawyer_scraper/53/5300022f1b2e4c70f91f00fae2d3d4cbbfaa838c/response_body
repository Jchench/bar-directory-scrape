<script>
	jQuery(document).ready(function($){	
		$(document).ready(function() {
			$('.skip-link').bind('keypress', function(event){
				if(event.which === 13) {
					$('main').get(0).focus();
					$('main').addClass('focus');
  					console.log('Focused');
				}
			});
			
			$('.skip-link').click(function(){
				$('main').get(0).focus();
				$('main').addClass('focus');
				console.log('Focused');
			});
		});				
	});
</script>
