    <script type="text/javascript">
    //<![CDATA[
        try {
            Sys.Net.WebRequestManager.add_invokingRequest(addCanaryToken);
        }
        catch (e) { }

        function addCanaryToken(sender, networkRequestEventArgs) {
            try {
                var headers = networkRequestEventArgs.get_webRequest().get_headers();
                var tokenName = 'canary';
                if (!headers[tokenName])
                    headers[tokenName] = '856cc61f-29ed-6201-2a3a-454895ce87f0';
            }
            catch (e) { }
        }
    //]]>
    </script>
    
    <script type="text/javascript">
        //<![CDATA[
        $R(function () {
            // check if cookie is enabled
            if ($R.areCookiesEnabled()) {
                $R("#NoCookiesMessageBox").collapse();
            }
            else {
                $R("#NoCookiesMessageBox").show();
                return;
            }

            window.smallViewport = false;
            fluidlayout.registerFunc(function(isMobile){
                window.smallViewport = isMobile ? true : false;
            });
            fluidlayout.apply();

            $R(window).resize(function(){
                updateLayout();
            });
            updateLayout();

            $R.recordScreenResolution();
            if ($R('#LogonPanel').length == 1) {
                // Both checking for sign out and language selector is logon page only.
                RemoteAccessWindowsPool.TryCheck(LogonResources.WindowsPoolKey, LogonResources.ClearSessionUrl);
                document.cookie = "suppressSharedFolderReadonlyNotification=0;path=/;";
                if ($R('#TimeoutPanel').length == 1 && !RemoteAccessWindowsPool.IsEmpty(LogonResources.WindowsPoolKey)) {
                    // For logon page used for timeout warning page
                    $R('.main').css('display', 'none');
                    $R('.timeout-panel').css('display', 'block');
                }
                else {
                    // For common logon pages
                    if (LogonResources.ShowLanguageList) {
                        $R('.language-list').menu({
                            showAbove: true,
                            autoChevron: false
                        });
                    }
                    $R('#LogonPanel').css('display', 'block');
                    $R('#LinksPanel').css('display', 'block');
                    $R('#LogonPanel').bind('keypress', function (e) {
                        if (e.keyCode == 13) { $R('#LogonButton').click(); return false; }
                    });
                    $R('#UserNameText').watermark(LogonResources.Username, 'watermark-txt', 'normal-txt', { keepWatermarkOnFocus: true });
                    $R('#UserNameText').attr('aria-label', LogonResources.Username);
                    $R('#PasswordText').watermark(LogonResources.Password, 'watermark-txt', 'normal-txt', { keepWatermarkOnFocus: true });
                    $R('#PasswordText').attr('aria-label', LogonResources.Password);
                    // Queue the focus event to allow UI changes from watermark to become effective first.
                    setTimeout(function () {
                        if ($R('#UserNameText').val() == 0) $R('#UserNameText').focus();
                        else $R('#PasswordText').focus();
                    }, 1);
                }
            }
            else if ($R('#ChangePasswordPanel').length == 1) {
                $R('#ViewSwitch').hide();
                $R('#ChangePasswordPanel').css('display', 'block');
                $R('#ChangePasswordPanel').bind('keypress', function (e) {
                    if (e.keyCode == 13) { $R('#ChangePasswordButton').click(); return false; }
                });
                $R('#OldPassword').watermark(LogonResources.OldPassword, 'watermark-txt', 'normal-txt', { keepWatermarkOnFocus: true });
                $R('#OldPassword').attr('aria-label', LogonResources.OldPassword);
                $R('#NewPassword').watermark(LogonResources.NewPassword, 'watermark-txt', 'normal-txt', { keepWatermarkOnFocus: true });
                $R('#NewPassword').attr('aria-label', LogonResources.NewPassword);
                $R('#ConfirmPassword').watermark(LogonResources.ConfirmPassword, 'watermark-txt', 'normal-txt', { keepWatermarkOnFocus: true });
                $R('#ConfirmPassword').attr('aria-label', LogonResources.ConfirmPassword);
                setTimeout(function () { $R('#OldPassword').focus(); }, 1);
            }
        });
        function showCover() {
            $R('#cover').css('display', 'block');
            $R('.main').find('a,input').each(function() { this.tabIndex = -1; });
            $R('.main').blur();
        }

        function updateLayout() {
            if (window.smallViewport){
                $R("#LoginContainer").css("padding-top", "");
            }
            else{
                var clientHeight = $R(".footer").offset().top;
                var height = $R("#LoginContainer").outerHeight();
                $R("#LoginContainer").css("padding-top", (clientHeight - height) / 3 + "px");
            }
            
        }
        //]]>
    </script>

